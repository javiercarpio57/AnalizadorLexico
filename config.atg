COMPILER Ejemplo
(.
/*--------------Scanner Specification----------*/
.)

CHARACTERS
tab = CHR(9).
eol = CHR(10).
blanco = eol + CHR(13)+tab.
letter = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ".
digit= "0123456789".
hexdigit= digit+"ABCDEF".

KEYWORDS
if1 = "if".
while="while".

TOKENS
id= letter{letter} EXCEPT KEYWORDS.
number = digit{digit}.
hexnumber = hexdigit{hexdigit}"(H)".
white = blanco{blanco}.

PRODUCTIONS

END Ejemplo